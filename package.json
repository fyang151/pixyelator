{
  "name": "pixyelator",
  "version": "1.1.67",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "build": "node scripts/build.js",
    "test:dist": "npm run build && cross-env TEST_DIST=true playwright test",
    "release": "node scripts/release.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "cross-env": "^7.0.3",
    "http-server": "^14.1.1",
    "terser": "^5.44.1"
  },
  "keywords": [
    "image",
    "pixelation",
    "canvas",
    "web-workers"
  ],
  "author": "fyang151",
  "license": "MIT",
  "description": "Web based pixelation library with custom pixel quantities.",
  "main": "dist/index.min.js",
  "exports": {
    ".": {
      "import": "./dist/index.min.js",
      "default": "./dist/index.min.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fyang151/pixyelator.git"
  },
  "bugs": {
    "url": "https://github.com/fyang151/pixyelator/issues"
  },
  "homepage": "https://github.com/fyang151/pixyelator#readme"
}
